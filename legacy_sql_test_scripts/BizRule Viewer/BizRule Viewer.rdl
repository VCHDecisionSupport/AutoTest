    <DataSet Name="ObjectNames">
      <Query>
        <DataSourceName>DQMF</DataSourceName>
        <CommandText>SELECT DatabaseName, ObjectSchemaName, ObjectPhysicalName
FROM DQMF.dbo.MD_Object AS obj
