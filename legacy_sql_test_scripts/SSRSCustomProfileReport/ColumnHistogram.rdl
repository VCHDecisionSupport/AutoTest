  <DataSets>
    <DataSet Name="ColumnHistogram">
      <Query>
        <DataSourceName>SsmsContext</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@column_name">
            <Value>=Parameters!column_name.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@DatabaseName">
            <Value>=Parameters!DatabaseName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@row_count">
            <Value>=Parameters!row_count.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@ObjectName">
            <Value>=Parameters!ObjectName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @sql nvarchar(1000);

SET @sql = FORMATMESSAGE('
SELECT %s AS value, COUNT(*) AS frequency, 1.*COUNT(*)/%i AS relative_frequency
FROM %s.%s
GROUP BY %s
HAVING 1.*COUNT(*)/%i &gt; 0.001
', @column_name, @row_count, @DatabaseName, @ObjectName, @column_name, @row_count)

--PRINT @sql;
EXEC(@sql);</CommandText>
      </Query>
      <Fields>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="frequency">
          <DataField>frequency</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="relative_frequency">
          <DataField>relative_frequency</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
